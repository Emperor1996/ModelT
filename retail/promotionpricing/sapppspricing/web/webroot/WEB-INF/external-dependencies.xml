<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.hybris.platform</groupId>
	<artifactId>sapppspricing</artifactId>
	<version>5.0.0.0-SNAPSHOT</version>

	<packaging>jar</packaging>

	<properties>
		<version.pps>3.0.16</version.pps>
		<version.jackson>2.9.5</version.jackson>
	</properties>

	<repositories>
		<repository>
			<id>sap-nexus-snapshot</id>
			<name>SAP Nexus Snapshot</name>
			<url>http://nexus.wdf.sap.corp:8081/nexus/content/repositories/deploy.snapshots</url>
			<snapshots>
				<updatePolicy>always</updatePolicy>
			</snapshots>
			<releases>
				<enabled>false</enabled>
			</releases>
		</repository>
		<repository>
			<id>sap-nexus-release</id>
			<name>SAP Nexus Release</name>
			<url>http://nexus.wdf.sap.corp:8081/nexus/content/repositories/build.releases</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

	<dependencies>
		<!-- PPS dependencies - binaries -->
		<dependency>
			<groupId>com.sap.retail.ppservice</groupId>
			<artifactId>ppengine-idocinbound</artifactId>
			<version>${version.pps}</version>
		</dependency>

		<!-- PPS dependencies - sources -->
		<dependency>
			<groupId>com.sap.retail.ppservice</groupId>
			<artifactId>ppengine-idocinbound</artifactId>
			<version>${version.pps}</version>
			<classifier>sources</classifier>
		</dependency>

		<!-- PPS dependencies - javadoc -->
		<dependency>
			<groupId>com.sap.retail.ppservice</groupId>
			<artifactId>ppengine-idocinbound</artifactId>
			<version>${version.pps}</version>
			<classifier>javadoc</classifier>
		</dependency>

		<!-- Misc -->
		<dependency>
			<groupId>com.fasterxml.jackson.dataformat</groupId>
			<artifactId>jackson-dataformat-xml</artifactId>
			<version>${version.jackson}</version>
		</dependency>
	</dependencies>
</project>
